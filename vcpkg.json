        { "name": "sdl3",  "version": "3.2.18",  "port-version": 0 },
        { "name": "glad",  "version": "0.1.36",  "port-version": 0 },
        { "name": "spdlog","version": "1.15.3",  "port-version": 0 },
        { "name": "imgui", "version": "1.92.0",  "port-version": 0 },
        { "name": "entt",  "version": "3.15.0",  "port-version": 0 }
    ]
}
